{
  "presets": [
    "@babel/env",
    "@babel/react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", {"loose": true}],
    [
      "module-resolver", {
        "alias": {
          "components/*": "./components",
          "views/*": "./src/components",
          "assets/*": "./src/assets"
        }
      }
    ]
  ]
}
